{
  "functions": {
    "source": "dist/app",
    "predeploy": [
      "npm run lint",
      "npm run build",
      "npm run copy",
      "npm run install-deps"
    ]
  },
  "hosting": {
    "public": "dist/public",
    "rewrites": [
      {
        "source": "**/**",
        "function": "application"
      }
    ],
    "predeploy": "npm run copy:public"
  }
}
